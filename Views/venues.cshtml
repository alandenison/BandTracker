<h1>Venues</h1>
@foreach (var venue in @Model)
{
<form action = "/venue-delete" method="post">
  <li><a href="/venue/@venue.GetId()">@venue.GetName()</a><input hidden name="venue-id" value=@venue.GetId()>
  <button type="submit" name="button">delete</button></li>
</form>
}

<Form action = "add-venue" method = "post">
  <label for="venue-name">Venue Name</label>
  <input type="text" name="venue-name">
  <button type="submit" name="name">add</button>
</form>
<a href="/">home</a>
<form action="/delete-all-venues" method="post">
  <button type="submit">Delete All</button>
</form>
